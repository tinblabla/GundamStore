@model List<Model.EF.Menu>



<div class="collapse navbar-collapse" id="myNavbar">
    @{
        var session = (UserLogin)Session[GundamShop.Common.CommonConstants.USER_SESSION];
    }
    @if (session == null)
    {
        <ul class="nav navbar-nav">
            @foreach (var item in Model)
            {
                <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>
            }
        </ul>

    }
    else
    {
        <ul class="nav navbar-nav">
            <li style="color: white;">Xin Chào, @session.UserName</li>
            <li>@Html.ActionLink("Đăng Xuất", "Logout", "User")</li>
        </ul>
    }
</div>

<div class="clear"></div>
